<script src="https://widget.cloudinary.com/v2.0/global/all.js" type="text/javascript">  
</script>
<script>
    
document.getElementById("upload_widget_opener").addEventListener("click", function() {
        
       cloudinary.openUploadWidget({ 
            cloud_name: 'dmupfimgd',
            upload_preset: 'ooq9e9f5',
            cropping: 'true',
            //cropping_coordinates_mode: "custom",
            croppingAspectRatio: 1,
            croppingDefaultSelectionRatio: 1
        },
                function(error, result) {
                   console.log(error, result)
                   console.log(result.info.url.event)
                   console.log(result.info.success)
                    // Push URL into text input
                    document.getElementById('url_text').value = result.info.public_id
                    document.getElementById('url_format').value = result.info.format
                    })

        }, false)
    
</script>