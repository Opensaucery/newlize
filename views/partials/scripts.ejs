<script src="https://widget.cloudinary.com/global/all.js" type="text/javascript">  
</script>
<script>
    

        const myWidget = cloudinary.createUploadWidget({ 
            cloud_name: 'dmupfimgd',
            upload_preset: 'ooq9e9f5',
            cropping: 'server',
            cropping_coordinates_mode: "custom",
            croppingAspectRatio: 0.9,
            croppingDefaultSelectionRatio: 0.9
        },
                function(error, result) {
                    console.log(error, result)
                    // Push URL into text input
                    document.getElementById('url_text').value = result[0].url;
                });
                document.getElementById("upload_widget_opener").addEventListener("click", function() {
                    myWidget.open()
                }, false)
    
</script>